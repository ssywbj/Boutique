<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--按下状态-->
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/keyboard_key_radius"/>
            <solid android:color="#9BA6B1"/>
        </shape>
    </item>
    <!--默认状态-->
    <item>
        <shape>
            <corners android:radius="@dimen/keyboard_key_radius"/>
            <solid android:color="#94A0AB"/>
        </shape>
    </item>
</selector>