<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--按下状态-->
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/keyboard_key_radius"/>
            <solid android:color="#768694"/>
        </shape>
    </item>
    <!--默认状态-->
    <item>
        <shape>
            <corners android:radius="@dimen/keyboard_key_radius"/>
            <solid android:color="#83919D"/>
        </shape>
    </item>
</selector>